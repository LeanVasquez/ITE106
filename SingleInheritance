class Animal {
	public void walk() {
		System.out.println("Animals walks");
	}
}
class Puppy extends Animal {
	public void walk() {
		System.out.println("Puppy walks");
	}	
}

public class SingleInheritance {
	public static void main(String[] args) {
		Puppy Mitchi = new Puppy();
		Mitchi.walk();
	}
}
